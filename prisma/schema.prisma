datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Goonsdetail {
  id        Int      @id @default(autoincrement())
  location  String
  ipAddress String   @default("none")
  time      DateTime
  createdAt DateTime @default(now())
}
